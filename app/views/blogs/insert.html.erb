<h1>hello</h1>
<%= form_with(model: @product,url: blogs_create_path, local: true) do |f| %>
  <p>  
    <%= f.text_field :product_name %>
  </p>

 
  <p>
  
    <%= f.number_field :product_price %>
  </p>

  <%= f.grouped_collection_select(:subcategory_id,@category_list,:subcategories,:category_name,:id,:subcat_name,{}, { :multiple => true, :size => 5,:class=>'chzn-select' })%>

   <p>
    <%= f.submit %>
  </p>
 
<% end %>

<%= javascript_pack_tag 'choosen'%>

